{
  "differential_privacy": {
    "enabled": true,
    "epsilon": 1.0,
    "delta": 1e-5,
    "sensitivity": 1.0,
    "clip_gradients": true,
    "max_gradient_norm": 1.0,
    "noise_distribution": "gaussian"
  },
  "secure_aggregation": {
    "enabled": false,
    "method": "dummy_noise",
    "num_dummy_updates": 2
  },
  "device_attestation": {
    "enabled": true,
    "require_signature": true,
    "signature_algorithm": "ECDSA",
    "key_size": 256
  },
  "privacy_budget": {
    "total_epsilon": 10.0,
    "track_per_device": true,
    "reset_on_model_update": false
  },
  "outlier_detection": {
    "enabled": true,
    "method": "statistical",
    "z_score_threshold": 3.0,
    "reject_outliers": true
  }
}

